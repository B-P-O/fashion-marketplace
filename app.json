{
  "expo": {
    "name": "FashionMarketPlace",
    "slug": "fashion-marketplace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "com.fashion.marketplace",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["ios", "android", "web"],
    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fashion.marketPlace"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon.png",
        "backgroundColor": "#228B22" 
      },
      "package": "com.fashion.marketPlace"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/icon.png"
    },
    "splash": {  
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#228B22"  
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}